# Supabase Configuration
# Copy this file to .env and fill in your Supabase project credentials
# Get these values from: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api

VITE_SUPABASE_URL=https://YOUR_PROJECT.supabase.co
VITE_SUPABASE_PUBLISHABLE_DEFAULT_KEY=your-publishable-default-key-here

# For webpack builds (if not using Vite)
SUPABASE_URL=https://YOUR_PROJECT.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here

# For seed script only (service role key - keep secret!)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here
